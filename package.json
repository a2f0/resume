{
  "name": "resume",
  "version": "0.1.124",
  "private": true,
  "type": "module",
  "engines": {
    "node": "=18.18.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "scripts": {
    "server": "next dev -p 4000",
    "server-test": "next dev -p 4001",
    "test": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm --no-warnings' npx wdio run wdio.shared.conf.ts",
    "test-headless": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm --no-warnings' npx wdio run wdio.headless.conf.ts",
    "ci": "start-server-and-test server-test http-get://localhost:4001 test",
    "ci-headless": "start-server-and-test server-test http-get://localhost:4001 test-headless",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint .",
    "compile": "tsc",
    "fix": "eslint . --fix",
    "prepare": "npm run compile",
    "unit": "vitest --no-watch --environment jsdom"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.7",
    "color": "4.2.3",
    "eslint-config-next": "14.2.5",
    "jspdf": "2.5.1",
    "next": "14.2.5",
    "pdfobject": "2.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "styled-components": "6.1.12",
    "ts-node": "10.9.2",
    "wdio-video-reporter": "5.2.0"
  },
  "devDependencies": {
    "@a2f0/check-for-unpinned-dependencies": "1.0.4",
    "@puppeteer/browsers": "2.3.1",
    "@testing-library/react": "16.0.0",
    "@types/color": "3.0.6",
    "@types/mocha": "10.0.7",
    "@types/node": "22.4.2",
    "@types/pdfobject": "2.2.5",
    "@types/react": "18.3.4",
    "@types/react-redux": "7.1.33",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@wdio/cli": "9.0.6",
    "@wdio/local-runner": "9.0.6",
    "@wdio/mocha-framework": "9.0.6",
    "@wdio/spec-reporter": "9.0.4",
    "canvas": "2.11.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "expect-webdriverio": "4.15.1",
    "jsdom": "24.1.1",
    "prettier": "3.3.3",
    "start-server-and-test": "2.0.5",
    "typescript": "5.5.4",
    "vercel": "36.0.0",
    "vitest": "2.0.5",
    "wdio-chromedriver-service": "8.1.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.21.0"
  }
}
