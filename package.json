{
  "name": "resume",
  "version": "0.1.110",
  "private": true,
  "type": "module",
  "engines": {
    "node": "=18.17.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "scripts": {
    "server": "next dev -p 4000",
    "server-test": "next dev -p 4001",
    "test": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm' npx wdio run wdio.shared.conf.ts",
    "test-headless": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm' npx wdio run wdio.headless.conf.ts",
    "ci": "start-server-and-test server-test http-get://localhost:4001 test",
    "ci-headless": "start-server-and-test server-test http-get://localhost:4001 test-headless",
    "build": "next build",
    "start": "next start",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "color": "4.2.3",
    "eslint-config-next": "14.0.4",
    "jspdf": "2.5.1",
    "next": "14.0.4",
    "pdfobject": "2.2.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2",
    "redux": "4.2.1",
    "styled-components": "6.0.7",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@a2f0/check-for-unpinned-dependencies": "1.0.4",
    "@puppeteer/browsers": "1.9.0",
    "@types/color": "3.0.3",
    "@types/mocha": "10.0.1",
    "@types/node": "20.8.0",
    "@types/pdfobject": "2.2.3",
    "@types/react": "18.2.20",
    "@types/react-redux": "7.1.25",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@wdio/cli": "8.26.1",
    "@wdio/local-runner": "8.26.1",
    "@wdio/mocha-framework": "8.24.12",
    "@wdio/spec-reporter": "8.24.12",
    "eslint": "8.48.0",
    "eslint-plugin-import": "2.28.1",
    "expect-webdriverio": "4.2.7",
    "gts": "5.2.0",
    "start-server-and-test": "2.0.3",
    "typescript": "5.1.6",
    "vercel": "32.3.1",
    "wdio-chromedriver-service": "8.1.1"
  }
}
