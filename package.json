{
  "name": "resume",
  "version": "0.1.109",
  "private": true,
  "type": "module",
  "engines": {
    "node": "=16.15.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "scripts": {
    "server": "next dev -p 4000",
    "server-test": "next dev -p 4001",
    "test": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm' npx wdio run wdio.shared.conf.ts",
    "test-headless": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm' npx wdio run wdio.headless.conf.ts",
    "ci": "start-server-and-test server-test http-get://localhost:4001 test",
    "ci-headless": "start-server-and-test server-test http-get://localhost:4001 test-headless",
    "build": "next build",
    "start": "next start",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.2",
    "@wdio/cli": "8.3.5",
    "color": "4.2.3",
    "eslint-config-next": "13.1.6",
    "jspdf": "2.5.1",
    "next": "13.1.6",
    "pdfobject": "2.2.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@a2f0/check-for-unpinned-dependencies": "1.0.4",
    "@types/color": "3.0.3",
    "@types/mocha": "10.0.1",
    "@types/node": "18.13.0",
    "@types/pdfobject": "2.2.3",
    "@types/react": "18.0.28",
    "@types/react-redux": "7.1.25",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@wdio/local-runner": "8.3.5",
    "@wdio/mocha-framework": "8.3.0",
    "@wdio/spec-reporter": "8.3.0",
    "chromedriver": "109.0.0",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "expect-webdriverio": "4.1.1",
    "gts": "3.1.1",
    "start-server-and-test": "1.15.3",
    "typescript": "4.9.5",
    "vercel": "28.15.3",
    "wdio-chromedriver-service": "8.0.1"
  }
}
