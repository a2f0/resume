{
  "name": "resume",
  "version": "0.1.124",
  "private": true,
  "type": "module",
  "engines": {
    "node": "=18.17.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "scripts": {
    "server": "next dev -p 4000",
    "server-test": "next dev -p 4001",
    "test": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm --no-warnings' npx wdio run wdio.shared.conf.ts",
    "test-headless": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm --no-warnings' npx wdio run wdio.headless.conf.ts",
    "ci": "start-server-and-test server-test http-get://localhost:4001 test",
    "ci-headless": "start-server-and-test server-test http-get://localhost:4001 test-headless",
    "build": "next build",
    "start": "next start",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "unit": "vitest --no-watch --environment jsdom"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.3",
    "color": "4.2.3",
    "eslint-config-next": "14.2.3",
    "jspdf": "2.5.1",
    "next": "14.2.3",
    "pdfobject": "2.3.0",
    "react": "18.3.1",
    "react-dom": "18.2.0",
    "react-redux": "9.1.1",
    "redux": "5.0.1",
    "styled-components": "6.1.9",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@a2f0/check-for-unpinned-dependencies": "1.0.4",
    "@puppeteer/browsers": "2.2.3",
    "@testing-library/react": "15.0.5",
    "@types/color": "3.0.6",
    "@types/mocha": "10.0.6",
    "@types/node": "20.12.7",
    "@types/pdfobject": "2.2.5",
    "@types/react": "18.3.1",
    "@types/react-redux": "7.1.33",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@wdio/cli": "8.36.1",
    "@wdio/local-runner": "8.36.1",
    "@wdio/mocha-framework": "8.36.1",
    "@wdio/spec-reporter": "8.36.1",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "expect-webdriverio": "4.12.2",
    "gts": "5.3.0",
    "jsdom": "24.0.0",
    "start-server-and-test": "2.0.3",
    "typescript": "5.4.5",
    "vercel": "34.1.7",
    "vitest": "1.5.3",
    "wdio-chromedriver-service": "8.1.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.17.2"
  }
}
