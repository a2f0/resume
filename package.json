{
  "name": "resume",
  "version": "0.1.115",
  "private": true,
  "type": "module",
  "engines": {
    "node": "=18.17.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "scripts": {
    "server": "next dev -p 4000",
    "server-test": "next dev -p 4001",
    "test": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm' npx wdio run wdio.shared.conf.ts",
    "test-headless": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm' npx wdio run wdio.headless.conf.ts",
    "ci": "start-server-and-test server-test http-get://localhost:4001 test",
    "ci-headless": "start-server-and-test server-test http-get://localhost:4001 test-headless",
    "build": "next build",
    "start": "next start",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "unit": "vitest --no-watch --environment jsdom"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "color": "4.2.3",
    "eslint-config-next": "14.0.4",
    "jspdf": "2.5.1",
    "next": "14.0.4",
    "pdfobject": "2.2.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2",
    "redux": "5.0.1",
    "styled-components": "6.1.6",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@a2f0/check-for-unpinned-dependencies": "1.0.4",
    "@puppeteer/browsers": "1.9.1",
    "@types/color": "3.0.6",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.6",
    "@types/pdfobject": "2.2.5",
    "@types/react": "18.2.47",
    "@types/react-redux": "7.1.33",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@wdio/cli": "8.27.1",
    "@wdio/local-runner": "8.27.0",
    "@wdio/mocha-framework": "8.27.0",
    "@wdio/spec-reporter": "8.27.0",
    "canvas": "2.11.2",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "expect-webdriverio": "4.8.0",
    "gts": "5.2.0",
    "jsdom": "23.2.0",
    "start-server-and-test": "2.0.3",
    "typescript": "5.3.3",
    "vercel": "33.0.2",
    "vitest": "1.1.3",
    "wdio-chromedriver-service": "8.1.1"
  }
}
